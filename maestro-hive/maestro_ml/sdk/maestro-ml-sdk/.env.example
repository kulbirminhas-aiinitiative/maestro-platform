# Maestro ML SDK Configuration
# Copy this file to .env and update with your values

# MLflow Configuration
MAESTRO_ML_MLFLOW_URI=http://localhost:5000
# MAESTRO_ML_MLFLOW_REGISTRY_URI=  # Optional, defaults to MLFLOW_URI

# Kubernetes Configuration
MAESTRO_ML_NAMESPACE=ml-platform
# MAESTRO_ML_KUBE_CONTEXT=  # Optional
# MAESTRO_ML_KUBE_CONFIG_PATH=~/.kube/config  # Optional

# API Configuration (if using Maestro ML Platform API)
# MAESTRO_ML_API_URL=http://localhost:8000
# MAESTRO_ML_API_KEY=your-api-key-here

# Timeouts
MAESTRO_ML_REQUEST_TIMEOUT=30
MAESTRO_ML_TRAINING_JOB_TIMEOUT=3600

# Feature Flags
MAESTRO_ML_VERIFY_SSL=true
MAESTRO_ML_VERBOSE=false
