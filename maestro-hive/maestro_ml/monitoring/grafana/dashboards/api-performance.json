{
  "dashboard": {
    "title": "Maestro ML - API Performance",
    "tags": ["maestro", "api", "performance"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "Request Rate by Endpoint",
        "targets": [
          {
            "expr": "sum by (endpoint) (rate(maestro_http_requests_total[5m]))",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ],
        "stack": true
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "Request Latency by Percentile",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(maestro_http_request_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(maestro_http_request_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(maestro_http_request_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "type": "graph",
        "title": "Error Rate by Endpoint",
        "targets": [
          {
            "expr": "sum by (endpoint, error_type) (rate(maestro_api_errors_total[5m]))",
            "legendFormat": "{{endpoint}}: {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Errors/sec"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.1], "type": "gt"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "name": "High Error Rate Alert"
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "type": "graph",
        "title": "Requests in Progress",
        "targets": [
          {
            "expr": "sum by (endpoint) (maestro_http_requests_in_progress)",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "In Progress"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "type": "graph",
        "title": "Status Code Distribution",
        "targets": [
          {
            "expr": "sum by (status) (rate(maestro_http_requests_total[5m]))",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ],
        "stack": true
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "type": "graph",
        "title": "Rate Limit Events",
        "targets": [
          {
            "expr": "sum by (identifier_type) (rate(maestro_rate_limit_exceeded_total[5m]))",
            "legendFormat": "{{identifier_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Events/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 24},
        "type": "table",
        "title": "Endpoint Performance Summary",
        "targets": [
          {
            "expr": "sum by (endpoint) (rate(maestro_http_requests_total[5m]))",
            "format": "table",
            "instant": true,
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum by (endpoint, le) (rate(maestro_http_request_duration_seconds_bucket[5m])))",
            "format": "table",
            "instant": true,
            "refId": "B"
          },
          {
            "expr": "sum by (endpoint) (rate(maestro_api_errors_total[5m]))",
            "format": "table",
            "instant": true,
            "refId": "C"
          }
        ],
        "transformations": [
          {
            "id": "merge"
          },
          {
            "id": "organize",
            "options": {
              "renameByName": {
                "endpoint": "Endpoint",
                "Value #A": "Request Rate",
                "Value #B": "p95 Latency",
                "Value #C": "Error Rate"
              }
            }
          }
        ]
      }
    ]
  }
}
