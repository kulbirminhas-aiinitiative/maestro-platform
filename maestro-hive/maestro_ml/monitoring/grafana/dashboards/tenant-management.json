{
  "dashboard": {
    "title": "Maestro ML - Tenant Management",
    "tags": ["maestro", "tenant", "multi-tenancy"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "1m",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "type": "graph",
        "title": "Quota Usage by Resource Type",
        "targets": [
          {
            "expr": "maestro_tenant_quota_usage_ratio",
            "legendFormat": "{{tenant_id}}: {{resource_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Usage %", "max": 1, "min": 0},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 0.8, "colorMode": "critical", "op": "gt", "fill": true, "line": true}
        ]
      },
      {
        "id": 2,
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 8},
        "type": "stat",
        "title": "Active Users",
        "targets": [
          {
            "expr": "sum(maestro_tenant_active_users)",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 8},
        "type": "stat",
        "title": "Total Tenants",
        "targets": [
          {
            "expr": "count(maestro_tenant_active_users)",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 8},
        "type": "graph",
        "title": "Active Users by Tenant",
        "targets": [
          {
            "expr": "maestro_tenant_active_users",
            "legendFormat": "{{tenant_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Users"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 14},
        "type": "table",
        "title": "Tenant Resource Usage",
        "targets": [
          {
            "expr": "maestro_tenant_quota_usage_ratio",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "__name__": true, "instance": true, "job": true},
              "renameByName": {
                "tenant_id": "Tenant ID",
                "resource_type": "Resource Type",
                "Value": "Usage Ratio"
              }
            }
          }
        ],
        "options": {
          "sortBy": [
            {
              "displayName": "Usage Ratio",
              "desc": true
            }
          ]
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "type": "graph",
        "title": "Models per Tenant",
        "targets": [
          {
            "expr": "maestro_ml_models_total",
            "legendFormat": "{{tenant_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Model Count"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "type": "graph",
        "title": "API Requests by Tenant (Last Hour)",
        "targets": [
          {
            "expr": "sum by (tenant_id) (increase(maestro_ml_model_operations_total[1h]))",
            "legendFormat": "{{tenant_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Requests"},
          {"format": "short"}
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "tenant",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(maestro_tenant_active_users, tenant_id)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    }
  }
}
