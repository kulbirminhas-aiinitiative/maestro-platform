{
  "dashboard": {
    "title": "Maestro ML Platform - Overview",
    "tags": ["maestro", "ml", "overview"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "HTTP Request Rate",
        "targets": [
          {
            "expr": "rate(maestro_http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}} ({{status}})",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "HTTP Request Latency (p95)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(maestro_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 8},
        "type": "stat",
        "title": "Total Models",
        "targets": [
          {
            "expr": "sum(maestro_ml_models_total)",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 8},
        "type": "stat",
        "title": "Total Experiments (24h)",
        "targets": [
          {
            "expr": "increase(maestro_ml_experiments_total[24h])",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 8},
        "type": "stat",
        "title": "API Error Rate",
        "targets": [
          {
            "expr": "rate(maestro_api_errors_total[5m])",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {"value": 0, "color": "green"},
            {"value": 0.01, "color": "yellow"},
            {"value": 0.1, "color": "red"}
          ]
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 8},
        "type": "stat",
        "title": "Rate Limit Exceeded (24h)",
        "targets": [
          {
            "expr": "increase(maestro_rate_limit_exceeded_total[24h])",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
        "type": "graph",
        "title": "Model Operations by Type",
        "targets": [
          {
            "expr": "rate(maestro_ml_model_operations_total[5m])",
            "legendFormat": "{{operation}} ({{status}})",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Operations/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
        "type": "graph",
        "title": "Feature Discovery Duration",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(maestro_feature_discovery_duration_seconds_bucket[5m]))",
            "legendFormat": "{{operation}} (p95)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 22},
        "type": "graph",
        "title": "Tenant Quota Usage",
        "targets": [
          {
            "expr": "maestro_tenant_quota_usage_ratio",
            "legendFormat": "{{tenant_id}}: {{resource_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Usage Ratio", "max": 1, "min": 0},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.8], "type": "gt"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "name": "Tenant Quota Warning"
        }
      }
    ]
  }
}
